{
  "manifest_version": 2,
  "name": "PickyJSON Extension",
  "version": "${version}",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/main.js"]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "icons": {
    "128": "./img/icon.png"
  }
}